{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "out",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, max-age=0, s-maxage=0"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        },
        {
          "key": "X-Vercel-Cache-Control",
          "value": "no-cache"
        },
        {
          "key": "Surrogate-Control",
          "value": "no-store"
        },
        {
          "key": "Cache-Tag",
          "value": "v1-${now}"
        }
      ]
    }
  ],
  "github": {
    "silent": false,
    "autoAlias": true
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_BUILD_ID": "${now}"
    }
  }
}
